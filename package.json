{
  "name": "clean-react",
  "version": "1.0.1",
  "description": "Clean React using TDD, Clean Architecture and Typescript",
  "author": "Brendon Geronimo",
  "license": "ISC",
  "scripts": {
    "start": "webpack-dev-server --open",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
